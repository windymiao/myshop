<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>MyShop</display-name>
  <servlet-mapping>
  	<servlet-name>rv</servlet-name>
  	<url-pattern>/registerview</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginView</servlet-name>
    <url-pattern>/LoginView</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GoodsServlet</servlet-name>
    <url-pattern>/GoodsServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GoodsView</servlet-name>
    <url-pattern>/GoodsView</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>rv</servlet-name>
  	<servlet-class>com.yq.view.RegisterView</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.yq.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginView</servlet-name>
    <servlet-class>com.yq.view.LoginView</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GoodsServlet</servlet-name>
    <servlet-class>com.yq.servlet.GoodsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GoodsView</servlet-name>
    <servlet-class>com.yq.view.GoodsView</servlet-class>
  </servlet>




  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>